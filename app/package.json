    "expo-image-manipulator": "~10.2.0",
    "expo-image-picker": "~12.0.1",
    "expo-linking": "~3.0.0",
    "expo-notifications": "~0.14.0",
    "expo-permissions": "~13.1.0",
    "expo-updates": "0.13.2",
    "expo-yarn-workspaces": "2.0.0",
    "firebase": "8.10.0",
    "fp-ts": "2.11.8",
    "husky": "7.0.4",
    "immer": "9.0.12",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.0",
    "mockdate": "3.0.5",
    "moment": "2.29.1",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
